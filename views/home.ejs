<div class="h-full w-full flex p-4 overflow-auto">
  <% if (!messages || messages.length === 0) { %>
  <p>No messages yet...</p>
  <% } else { %> <% for (let i = 0; i < messages.length; i++) { %>
  <div class="w-12 h-24 border-2">
    <p><%= messages[i].content %></p>
  </div>
  <% } %> <% } %>
</div>
